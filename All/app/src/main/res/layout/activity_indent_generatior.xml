<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.indent.IndentGenerator">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TableRow
            android:id="@+id/row1"
            android:layout_width="285dp"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/simpleTextView"
                android:layout_width="113dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:textAlignment="textEnd"
                android:textColor="#000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/transports"
                android:layout_width="267dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="@string/balaji_transports"
                android:textColor="#000"
                android:textSize="20sp"
                android:textStyle="bold" />

        </TableRow>
        <TableRow
            android:id="@+id/date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/Datetext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="Date : "
                android:textColor="#000"
                android:textSize="20sp" />
            <TextView
                android:id="@+id/Datevalue"
                android:layout_width="238dp"
                android:layout_height="wrap_content"
                android:hint="Date Value"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp" />
        </TableRow>
        <TableRow
            android:id="@+id/Indent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/Indenttext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="No : "
                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/Indentvalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="Indent Number"/>
        </TableRow>
        <TableRow
            android:id="@+id/Unique"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/Uniquetext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="Unique ID : "
                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/Uniquevalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="Unique Number"/>
        </TableRow>

        <TableRow
            android:id="@+id/Phasesupply"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <TextView
                android:id="@+id/Phasesupplytext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="20sp" />
            <TextView
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="Please supply"
                android:textColor="#000"
                android:textSize="20sp"
                />
        </TableRow>

        <TableRow
            android:id="@+id/Diesel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/Dieseltext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                
                android:padding="10dp"
                android:text="Diesel : "

                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/Dieselvalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"

                
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="HSD"/>
        </TableRow>
        <TableRow
            android:id="@+id/Inwords"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/Inwordstext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                
                android:padding="10dip"
                android:text="In words : "

                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/Inwordsvalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="   "/>
        </TableRow>
        <TableRow
            android:id="@+id/VehicleNO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/VehicleNOtext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="VehicleNO : "
                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/VehicleNOvalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"

                
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="VehicleNO"/>
        </TableRow>
        <TableRow
            android:id="@+id/Bunk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/Bunktext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="Bunk : "
                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/Bunkvalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="Bunk"/>
        </TableRow>
        <TableRow
            android:id="@+id/Driver"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/Drivertext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="Driver : "
                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/Drivervalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="Driver"/>
        </TableRow>

        <TableRow
            android:id="@+id/IssuedBy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/IssuedBytext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="IssuedBy : "
                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/IssuedByvalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="IssuedBy"/>
        </TableRow>
        <TableRow
            android:id="@+id/Remarks"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/Remarkstext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="Remarks : "
                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/Remarksvalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp" />
        </TableRow>
        <TableRow
            android:id="@+id/BunkCash"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/BunkCashtext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text="BunkCash : "
                android:textColor="#000"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/BunkCashvalue"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp"
                android:hint="@string/bunkcash"/>
        </TableRow>
        <TableRow
            android:id="@+id/Empty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

            <TextView
                android:id="@+id/Emptytext"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:padding="10dip"
                android:text=""
                android:textColor="#000"
                android:textSize="20sp" />


            <Button
                android:id="@+id/Back"
                android:padding="10dip"
                android:layout_width="217dp"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:backgroundTint="@color/design_default_color_primary"
                android:text="@string/Back" />
        </TableRow>
        <TableRow
            android:id="@+id/Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

        </TableRow>

    </TableLayout>

</RelativeLayout>